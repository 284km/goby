class User {
    def initialize(name, age) {
        @name = name;
        @age = age;
    }

    def name {
        @name
    }

    def age {
        @age
    }

    def say_hi(user) {
        puts(@name + " says hi to " + user.name)
    }

    def self.sum_age(user1, user2) {
        user1.age + user2.age
    }
}

stan = User.new("Stan", 22);
john = User.new("John", 40);
puts(User.sum_age(stan, john));
stan.say_hi(john);